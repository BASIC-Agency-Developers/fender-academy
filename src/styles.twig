		<p><a class="btn btn-disabled" href="#">Select an answer to continue</a></p>

		<p><a class="btn btn-disabled btn-locked" href="#">Locked</a></p>

		<div class="vid">
			<video class="vid_content" width="640" height="360">
				<source src="/assets/vid/test.mp4" type="video/mp4" />
			</video>

			<div class="vid_controls">
				<button type="button" class="vid_btn vid_play"><span class="icon vid_icn">Play</span></button>
				<progress class="vid_progress" value="0" max="100"></progress>
				<span class="vid_time"></span>
				<div class="vid_volume">
					<button type="button" class="vid_btn vid_mute"><span class="icon vid_icn">Mute</span></button>
					<div class="vid_volbar">
						<input type="range" class="vid_vol" value="1" min="0" max="1" step=".1" orientation="vertical" />
					</div>
				</div>
				<button type="button" class="vid_btn vid_full"><span class="icon vid_icn">Full Screen</span></button>
			</div><!-- /.vid_controls -->
		</div><!-- /.vid -->

		<div class="canvIq" data-iq="110" data-goal="200" data-size="35" data-dots="no"></div>
		<div class="canvIq" data-iq="110" data-goal="200" data-size="106" data-dots="yes"></div>
	</div>
{% endblock %}
